{
  "name": "react-image-lazy-load",
  "version": "1.2.1",
  "description": "React Image Lazy Load plug-in with loader/placeholder image option - Forked from react-lazy-load plug-in - https://github.com/loktar00/react-lazy-load",
  "main": "./lib/LazyLoad.js",
  "scripts": {
    "build": "npm run build:lib && npm run build:umd && npm run build:umd:min",
    "build:lib": "babel src --out-dir lib",
    "build:umd": "webpack src/LazyLoad.jsx dist/LazyLoad.js --config webpack.config.development.js",
    "build:umd:min": "webpack src/LazyLoad.jsx dist/LazyLoad.min.js --config webpack.config.production.js",
    "clean": "rimraf lib dist",
    "lint": "eslint src/LazyLoad.jsx",
    "prepublish": "npm run clean && npm run build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vikash-bhardwaj/react-image-lazy-load.git"
  },
  "files": [
    "dist",
    "lib"
  ],
  "keywords": [
    "react",
    "reactjs",
    "react-component",
    "load",
    "lazy"
  ],
  "author": "Vikash Bhardwaj <vikasbhardwaj7@gmail.com>",
  "contributors": [
    "Sergey Laptev <iamsergeylaptev@gmail.com> (https://twitter.com/iamsergeylaptev)"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.2.1",
    "babel-eslint": "^4.1.5",
    "babel-jest": "^12.0.2",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^1.8.0",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-plugin-react": "^3.7.1",
    "jest-cli": "^12.0.2",
    "prop-types": "^15.6.1",
    "react": "^0.14.8",
    "react-addons-test-utils": "^0.14.8",
    "react-dom": "^0.14.8",
    "rimraf": "^2.4.4",
    "webpack": "^1.15.0"
  },
  "dependencies": {
    "babel-preset-stage-2": "^6.24.1",
    "eventlistener": "0.0.1",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0-0 || ^16.0.0",
    "react-dom": "^0.14.0 || ^15.0.0-0 || ^16.0.0"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react/",
      "<rootDir>/node_modules/react-dom/",
      "<rootDir>/node_modules/react-addons-test-utils/"
    ]
  }
}
